{
    "name": "kiriunin/laravel-apn-push",
    "description": "Push notifications to Apple devices for Laravel",
    "keywords": ["laravel", "apple", "push", "apn", "notifications"],
    "license": "MIT",
    "type": "library",

    "authors": [
        {
            "name": "Evgeniy Kiriunin",
            "email": "e@kiriunin.ru"
        }
    ],

    "require": {
        "php": "^7.2",
        "illuminate/support": "5.6.x",
        "apple/apn-push": "^3.0"
    },

	"require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },

    "suggest": {
        "web-token/jwt-key-mgmt": "For use JWT authenticator."
    },

    "autoload": {
        "psr-4": {
            "Kiriunin\\LaravelApnPush\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Kiriunin\\LaravelApnPush\\Tests\\": "tests/"
        }
    },

    "extra": {
        "laravel": {
            "providers": [
                "Kiriunin\\LaravelApnPush\\ApnServiceProvider"
            ],
            "aliases": {
                "Apn": "Kiriunin\\LaravelApnPush\\Facades\\Apn"
            }
        }
    },

    "config": {
        "bin-dir": "bin"
    }
}
